#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/suraj/development/online/node_modules/.pnpm/pdf-parse@2.4.5/node_modules/pdf-parse/bin/node_modules:/Users/suraj/development/online/node_modules/.pnpm/pdf-parse@2.4.5/node_modules/pdf-parse/node_modules:/Users/suraj/development/online/node_modules/.pnpm/pdf-parse@2.4.5/node_modules:/Users/suraj/development/online/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/suraj/development/online/node_modules/.pnpm/pdf-parse@2.4.5/node_modules/pdf-parse/bin/node_modules:/Users/suraj/development/online/node_modules/.pnpm/pdf-parse@2.4.5/node_modules/pdf-parse/node_modules:/Users/suraj/development/online/node_modules/.pnpm/pdf-parse@2.4.5/node_modules:/Users/suraj/development/online/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/cli.mjs" "$@"
else
  exec node  "$basedir/../../bin/cli.mjs" "$@"
fi
